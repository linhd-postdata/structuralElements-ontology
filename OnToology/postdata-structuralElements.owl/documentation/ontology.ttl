###  http://postdata.linhd.uned.es/ontology/postdata-structuralElements#belongsToLine
:belongsToLine rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf <http://purl.org/ontology/olo/core#ordered_list> ;
               owl:inverseOf :hasToken ;
               rdfs:domain :Token ;
               rdfs:range :Line ;
               rdfs:comment "This property refers to the Line (viewed as a list of tokens) the token belongs to."@en ;
               rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-structuralElements"@en ;
               rdfs:label "belongsToLine"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-structuralElements#belongsToLineList
:belongsToLineList rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf <http://purl.org/ontology/olo/core#ordered_list> ;
                   owl:inverseOf :hasLineItem ;
                   rdfs:domain :Line ;
                   rdfs:range :OrderedLineList ;
                   rdfs:comment "This property refers to the OrderedLineList , associated with a Redaction; the Line belongs to."@en ;
                   rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-structuralElements"@en ;
                   rdfs:label "belongsToLineList"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-structuralElements#belongsToStanza
:belongsToStanza rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf <http://purl.org/ontology/olo/core#ordered_list> ;
                 owl:inverseOf :hasLine ;
                 rdfs:domain :Line ;
                 rdfs:range :Stanza ;
                 rdfs:comment "This property refers to the Stanza (viewed as a list of lines) theLine belongs to."@en ;
                 rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-structuralElements"@en ;
                 rdfs:label "belongsToStanza"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-structuralElements#belongsToStanzaList
:belongsToStanzaList rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf <http://purl.org/ontology/olo/core#ordered_list> ;
                     owl:inverseOf :hasStanzaItem ;
                     rdfs:domain :Stanza ;
                     rdfs:range :OrderedStanzaList ;
                     rdfs:comment "This property refers to the OrderedStanzaList , associated with a Redaction; the Stanza belongs to."@en ;
                     rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-structuralElements"@en ;
                     rdfs:label "belongsToStanzaList"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-structuralElements#belongsToWord
:belongsToWord rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf <http://purl.org/ontology/olo/core#ordered_list> ;
               owl:inverseOf :hasSyllable ;
               rdfs:domain :Syllable ;
               rdfs:range :Word ;
               rdfs:comment "This property refers to the Word (viewed as a list of syllables) the Syllable belongs to."@en ;
               rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-structuralElements"@en ;
               rdfs:label "belongsToWord"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-structuralElements#containsMentionTo
:containsMentionTo rdf:type owl:ObjectProperty ;
                   rdfs:domain :Line ;
                   rdfs:comment "Agent or Place that is mentioned on the line"@en ;
                   rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-structuralElements"@en ;
                   rdfs:label "containsMentionTo"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-structuralElements#containsMentionToAgent
:containsMentionToAgent rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf :containsMentionTo ;
                        rdfs:range <http://postdata.linhd.uned.es/ontology/postdata-core#Agent> ;
                        rdfs:comment "Agent  that is mentioned on the line"@en ;
                        rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-structuralElements"@en ;
                        rdfs:label "containsMentionToAgent"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-structuralElements#containsMentionToPlace
:containsMentionToPlace rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf :containsMentionTo ;
                        rdfs:range <http://postdata.linhd.uned.es/ontology/postdata-core#Place> ;
                        rdfs:comment "Place that is mentioned on the line"@en ;
                        rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-structuralElements"@en ;
                        rdfs:label "containsMentionToPlace"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-structuralElements#hasFirstLine
:hasFirstLine rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              owl:inverseOf :isFirstLine ;
              rdfs:domain :Stanza ;
              rdfs:range :Line ;
              <http://purl.org/dc/elements/1.1/creator> "mluisadiez" ;
              rdfs:comment "This property points to the first line (an instance of Line) which form the current element under analysis" ;
              rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-structuralElements"@en ;
              rdfs:label "hasFirstLine"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-structuralElements#hasFirstStanza
:hasFirstStanza rdf:type owl:ObjectProperty ;
                owl:inverseOf :isFirstStanza ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( <http://postdata.linhd.uned.es/ontology/postdata-core#Redaction>
                                            :OrderedStanzaList
                                          )
                            ] ;
                rdfs:range :Stanza ;
                rdfs:comment "This property points to the first group of lines (an instance of Stanza) which form the current element under analysis"@en ;
                rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-structuralElements"@en ;
                rdfs:label "hasFirstStanza"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-structuralElements#hasFirstSyllable
:hasFirstSyllable rdf:type owl:ObjectProperty ;
                  owl:inverseOf :isFirstSyllable ;
                  rdfs:domain :Word ;
                  rdfs:range :Syllable .


###  http://postdata.linhd.uned.es/ontology/postdata-structuralElements#hasFirstToken
:hasFirstToken rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               owl:inverseOf :isFirstToken ;
               rdfs:domain :Line ;
               rdfs:range :Token ;
               <http://purl.org/dc/elements/1.1/creator> "mluisadiez" ;
               rdfs:comment "This property points to one of the words (an instance of Punctuation or Word) which form the current element under analysis"@en ;
               rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-structuralElements"@en ;
               rdfs:label "hasFirstToken"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-structuralElements#hasLastLine
:hasLastLine rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             owl:inverseOf :isLastLine ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( <http://postdata.linhd.uned.es/ontology/postdata-core#Redaction>
                                         :OrderedLineList
                                         :Stanza
                                       )
                         ] ;
             rdfs:range :Line ;
             rdfs:comment "This property points to the last line (an instance of Line) which form the current element under analysis"@en ;
             rdfs:label "hasLastLine"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-structuralElements#hasLastStanza
:hasLastStanza rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               owl:inverseOf :isLastStanza ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( <http://postdata.linhd.uned.es/ontology/postdata-core#Redaction>
                                           :OrderedStanzaList
                                         )
                           ] ;
               rdfs:range :Stanza ;
               rdfs:comment "This property points to the last Stanza  which form the current element under analysis"@en ;
               rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-structuralElements"@en ;
               rdfs:label "hasLastStanza"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-structuralElements#hasLastSyllable
:hasLastSyllable rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 owl:inverseOf :isLastSyllable ;
                 rdfs:domain :Word ;
                 rdfs:range :Syllable ;
                 rdfs:comment "This property points to the last Syllable which form the current element under analysis"@en ;
                 rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-structuralElements"@en ;
                 rdfs:label "hasLastSyllable"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-structuralElements#hasLastToken
:hasLastToken rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              owl:inverseOf :isLastToken ;
              rdfs:domain :Line ;
              rdfs:range :Token ;
              rdfs:comment "This property points to the last Token  which form the current element under analysis"@en ;
              rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-structuralElements"@en ;
              rdfs:label "hasLastToken"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-structuralElements#hasLine
:hasLine rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf <http://purl.org/ontology/olo/core#slot> ;
         rdfs:domain :Stanza ;
         rdfs:range :Line ;
         rdfs:comment "Line of a Stanza considered as list"@en ;
         rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-structuralElements"@en ;
         rdfs:label "hasLine"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-structuralElements#hasLineItem
:hasLineItem rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf <http://purl.org/ontology/olo/core#slot> ;
             rdfs:domain :OrderedLineList ;
             rdfs:range :Line ;
             rdfs:comment "Line of a OrderedLineList associated with a Redaction"@en ;
             rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-structuralElements"@en ;
             rdfs:label "hasLineItem"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-structuralElements#hasLineList
:hasLineList rdf:type owl:ObjectProperty ;
             owl:inverseOf :isLineListOfRedaction ;
             rdfs:domain <http://postdata.linhd.uned.es/ontology/postdata-core#Redaction> ;
             rdfs:range :OrderedLineList ;
             rdfs:comment "This property refers to the list of lines(the poem structured in lines) that the redaction contains to"@en ;
             rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-structuralElements/"@en ;
             rdfs:label "hasLineList"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-structuralElements#hasStanzaItem
:hasStanzaItem rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf <http://purl.org/ontology/olo/core#slot> ;
               rdfs:domain :OrderedStanzaList ;
               rdfs:range :Stanza ;
               rdfs:comment "Stanza of an OrderedStanzaList associated with a Redaction"@en ;
               rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-structuralElements"@en ;
               rdfs:label "hasStanzaItem"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-structuralElements#hasStanzaList
:hasStanzaList rdf:type owl:ObjectProperty ;
               owl:inverseOf :isStanzaListOfRedaction ;
               rdfs:domain <http://postdata.linhd.uned.es/ontology/postdata-core#Redaction> ;
               rdfs:range :OrderedStanzaList ;
               rdfs:comment "This property refers to the list of stanzas (the poem structured in stanzas) that the redaction contains to"@en ;
               rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-structuralElements/"@en ;
               rdfs:label "hasStanzaList"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-structuralElements#hasSyllable
:hasSyllable rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf <http://purl.org/ontology/olo/core#slot> ;
             rdfs:domain :Word ;
             rdfs:range :Syllable ;
             rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-structuralElements"@en ;
             rdfs:label "Syllable of a Word considered as list"@en ,
                        "hasSyllable"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-structuralElements#hasToken
:hasToken rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf <http://purl.org/ontology/olo/core#slot> ;
          rdfs:domain :Line ;
          rdfs:range :Token ;
          rdfs:comment "Token of a Line considered as list"@en ;
          rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-structuralElements"@en ;
          rdfs:label "hasToken"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-structuralElements#isCallOf
:isCallOf rdf:type owl:ObjectProperty ;
          rdfs:domain :Word ;
          rdfs:range :Word ;
          rdfs:comment "This property points to a previous Word (call) that rhymes with the current instance of Word"@en ;
          rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-structuralElements"@en ;
          rdfs:label "isCallOf"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-structuralElements#isEchoOf
:isEchoOf rdf:type owl:ObjectProperty ;
          rdfs:domain :Word ;
          rdfs:range :Word ;
          rdfs:comment "This property points to a following Word (echo) that that rhymes with the current instance of Word"@en ;
          rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-structuralElements"@en ;
          rdfs:label "isEchoOf"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-structuralElements#isFirstLine
:isFirstLine rdf:type owl:ObjectProperty ;
             rdfs:domain :Line ;
             rdfs:range :Stanza ;
             rdfs:comment "This property points to the group of lines (an instance of Stanza or an instance of OrderedLineList) of which the current item under analysis is its first line"@en ;
             rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-structuralElements"@en ;
             rdfs:label "isFirstLine"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-structuralElements#isFirstStanza
:isFirstStanza rdf:type owl:ObjectProperty ;
               rdfs:domain :Stanza ;
               rdfs:range <http://postdata.linhd.uned.es/ontology/postdata-core#Redaction> ;
               rdfs:comment "This property points to the text (an instance of OrderedStanzaList) of which the current item under analysis is its first line grouping"@en ;
               rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-structuralElements"@en ;
               rdfs:label "isFirstStanza"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-structuralElements#isFirstSyllable
:isFirstSyllable rdf:type owl:ObjectProperty ;
                 rdfs:domain :Syllable ;
                 rdfs:range :Word ;
                 rdfs:comment "This property points to an instance of Word of which the current item under analysis is its first Syllable"@en ;
                 rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-structuralElements"@en ;
                 rdfs:label "isFirstSyllable"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-structuralElements#isFirstToken
:isFirstToken rdf:type owl:ObjectProperty ;
              rdfs:domain :Token ;
              rdfs:range :Line ;
              rdfs:comment "This property points to an instance of Line of which the current item under analysis is its first Token" ;
              rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-structuralElements"@en ;
              rdfs:label "isFirstToken"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-structuralElements#isInflectedForm
:isInflectedForm rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 owl:inverseOf :isLemma ;
                 rdfs:domain :Word ;
                 rdfs:range :Word ;
                 <http://purl.org/dc/elements/1.1/creator> "mluisadiez" ;
                 rdfs:comment "This property points to its lemma (an instance of Word)"@en ;
                 rdfs:label "inflectedForm"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-structuralElements#isLastLine
:isLastLine rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:domain :Line ;
            rdfs:range [ rdf:type owl:Class ;
                         owl:unionOf ( <http://postdata.linhd.uned.es/ontology/postdata-core#Redaction>
                                       :OrderedLineList
                                       :Stanza
                                     )
                       ] ;
            rdfs:comment "This property points to Stanza or OrderedLineList of which the current item under analysis is its last Line"@en ;
            rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-structuralElements"@en ;
            rdfs:label "isLastLine"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-structuralElements#isLastStanza
:isLastStanza rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdfs:domain :Stanza ;
              rdfs:range [ rdf:type owl:Class ;
                           owl:unionOf ( <http://postdata.linhd.uned.es/ontology/postdata-core#Redaction>
                                         :OrderedStanzaList
                                       )
                         ] ;
              rdfs:comment "This property points to OrderedStanzaList of which the current item under analysis is its last Stanza"@en ;
              rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-structuralElements"@en ;
              rdfs:label "isLastStanza"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-structuralElements#isLastSyllable
:isLastSyllable rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                rdfs:domain :Syllable ;
                rdfs:range :Word ;
                rdfs:comment "This property points to Word of which the current item under analysis is its last Syllable"@en ;
                rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-structuralElements"@en ;
                rdfs:label "isLastSyllable"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-structuralElements#isLastToken
:isLastToken rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:domain :Token ;
             rdfs:range :Line ;
             rdfs:comment "This property points to Line of which the current item under analysis is its last Token"@en ;
             rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-structuralElements"@en ;
             rdfs:label "isLastToken"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-structuralElements#isLemma
:isLemma rdf:type owl:ObjectProperty ;
         rdfs:domain :Word ;
         rdfs:range :Word ;
         rdfs:comment "This property points to one of the inflected forms (an instance of Word) of the current headword (the Word at hand)"@en ;
         rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-structuralElements"@en ;
         rdfs:label "isLemma"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-structuralElements#isLineListOfRedaction
:isLineListOfRedaction rdf:type owl:ObjectProperty ;
                       rdfs:domain :OrderedLineList ;
                       rdfs:range <http://postdata.linhd.uned.es/ontology/postdata-core#Redaction> ;
                       rdfs:comment "This property refers to the Redaction that OrderedLineList belongs to"@en ;
                       rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-structuralElements/"@en ;
                       rdfs:label "isLineListOfRedaction"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-structuralElements#isStanzaListOfRedaction
:isStanzaListOfRedaction rdf:type owl:ObjectProperty ;
                         rdfs:domain :OrderedStanzaList ;
                         rdfs:range <http://postdata.linhd.uned.es/ontology/postdata-core#Redaction> ;
                         rdfs:comment "This property refers to the Redaction that OrderedStanzaList belongs to"@en ;
                         rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-structuralElements/"@en ;
                         rdfs:label "isStanzaListOfRedaction"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-structuralElements#isTheSameAs
:isTheSameAs rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:domain :Stanza ;
             rdfs:range :Stanza ;
             <http://purl.org/dc/elements/1.1/creator> "mluisadiez" ;
             rdfs:comment "This property points to another instance of Stanza which belongs to another work or ensemble but that has the same contents as the current item at hand"@en ;
             rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-structuralElements"@en ;
             rdfs:label "isTheSameAs"@en .


